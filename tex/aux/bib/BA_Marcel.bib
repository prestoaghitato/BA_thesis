	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {turn-taking, multimodal turn-taking},
	Pages = {10587--10592},
	Title = {Universals and cultural variation in turn-taking in conversation},
	Year = {2009},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1073/pnas.0903616106}}

