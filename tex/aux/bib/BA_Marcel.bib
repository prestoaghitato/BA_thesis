	Journal = {Ear and Hearing},
	Keywords = {reaction time},
	Language = {en-US},
	Month = apr,
	Number = {2},
	Pages = {92},
	Title = {Relationship between intensity and reaction time in normal-hearing infants and adults},
	Urldate = {2018-07-10},
	Volume = {23},
	Year = {2002}}

